{
  "name": "chord-analyzer",
  "private": true,
  "version": "0.0.1",
  "description": "Analyzes input from QWERTY or MIDI keyboard and displays detected chord",
  "author": "Clayton Watts",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others --names tsc,es-dev-server \"npm run tsc:watch\" \"es-dev-server --open --watch\"",
    "tsc:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cletusw/chord-analyzer.git"
  },
  "keywords": [
    "music-theory",
    "piano",
    "web-midi"
  ],
  "bugs": {
    "url": "https://github.com/cletusw/chord-analyzer/issues"
  },
  "homepage": "https://cletusw.github.io/chord-analyzer",
  "dependencies": {
    "lit-html": "^1.0.0",
    "lit-element": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "13.11.1",
    "es-dev-server": "^1.55.0",
    "typescript": "~3.8.2",
    "concurrently": "^5.1.0",
    "tslib": "^1.11.0"
  }
}
